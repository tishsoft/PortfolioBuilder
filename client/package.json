    "jquery": "^3.3.1",
    "marked": "^0.3.19",
    "materialize-css": "^0.100.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "2.0.8",
    "prismjs": "^1.15.0",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "querystring": "^0.2.0",
