{
  "name": "looseleaf-node",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build:prod": "./node_modules/.bin/webpack --mode production",
    "build:dev": "./node_modules/.bin/webpack --mode development",
    "prestart": "npm run build:prod",
    "start": "node server/run-prod.js",
    "start-server-dev": "nodemon server/run-dev.js --watch server --watch client/iso-middleware",
    "start-dev": "concurrently \"npm run start-server-dev\" \"npm run start-client\"",
    "build": "npm run build-client && npm run build-server",
    "build-server": "./node_modules/.bin/babel server/server.js -o server/build/main.bundle.js",
    "build-client": "cd  client && npm run build && cd ..",
    "start-server": "node server/run-prod.js",
    "start-server-prod": "forever start --minUptime 1000 --spinSleepTime 1000 -c babel-node server/server.js",
    "stop-server-prod": "forever stopall",
    "start-client": "node server/start-client.js",
    "install-all": "npm install && cd ./client && npm install && cd ..",
    "lint:server": "./node_modules/.bin/eslint server --ext .js, .jsx --fix",
    "test": "mocha --compilers js:babel-core/register"
  },
  "dependencies": {
    "arraywrap": "^1.0.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "csurf": "^1.9.0",
    "cuid": "^2.1.4",
    "dotenv": "^6.1.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-enforces-ssl": "^1.1.0",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "fs": "0.0.1-security",
    "gravatar-url": "^2.0.0",
    "helmet": "^3.15.0",
    "helmet-csp": "^2.7.1",
    "mongoose": "^5.3.13",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.1",
    "ms": "^2.1.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "source-map-support": "^0.5.9",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  }
}
